library(ggplot2)
setwd('C:\\Users\\kaley\\Documents\\college\\CDTerm24\\DataVis\\a2-DataVis-7Ways')
pdata <- read.csv("penglings.csv")
ggplot(pdata, aes(flipper_length_mm, body_mass_g, colour = species)) + geom_point(size=6, alpha = 0.8) +
xlab("Flipper Length(mm)") + ylab("Body Mass(g)") + scale_color_manual(values=c("#ED6A5A", "#05A8AA", "#8A89C0"))
